{
   "background": {
      "scripts": [
       "lib/Base.js",
       "lib/DOM.js",
       "lib/Async.js",
       "lib/md5.js",
       "lib/proto.js",
       "lib/utils.js",
       "lib/repository.js",
       "lib/models.js",
       "lib/background.js",
       "lib/menu.js"
      ]
   },
   "content_scripts": [ {
      "js": [
       "lib/Base.js",
       "lib/DOM.js",
       "lib/Async.js",
       "lib/Style.js",
       "lib/Signal.js",
       "lib/proto.js",
       "lib/utils.js",
       "lib/repository.js",
       "lib/extractors.js",
       "lib/userscripts.js",
       "lib/content.js"
      ],
      "css": ["styles/general.css"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
     "default_icon" : "skin/fork19.png",
     "default_title": "taberareloo",
     "default_popup": "popup.html"
   },
   "options_page": "options.html",
   "sandbox": {
     "pages": [
       "sandbox.html"
     ],
     "content_security_policy": "sandbox allow-scripts; 'unsafe-eval'"
   },
   "web_accessible_resources": [
      "styles/dashboard.css",
      "styles/reader.css",
      "skin/4u.ico",
      "skin/accept.png",
      "skin/arrow-down.png",
      "skin/arrow-up.png",
      "skin/audio.png",
      "skin/chromium.ico",
      "skin/clipp.ico",
      "skin/conversation.png",
      "skin/cross.png",
      "skin/default.png",
      "skin/disabled.png",
      "skin/empty.png",
      "skin/enabled.png",
      "skin/extension.gif",
      "skin/firefox.ico",
      "skin/fork.svg",
      "skin/fork128.png",
      "skin/fork16.png",
      "skin/fork19.png",
      "skin/fork32.png",
      "skin/fork64.png",
      "skin/google-bookmark.png",
      "skin/googleplus.ico",
      "skin/instapaper.png",
      "skin/item.ico",
      "skin/link.png",
      "skin/loading.gif",
      "skin/local.ico",
      "skin/mac/close.png",
      "skin/music.png",
      "skin/photo.png",
      "skin/pick-naver.png",
      "skin/quote.png",
      "skin/reblog.ico",
      "skin/regular.png",
      "skin/snap-bottom-left.png",
      "skin/snap-bottom-right.png",
      "skin/snap-top-left.png",
      "skin/snap-top-right.png",
      "skin/star.png",
      "skin/taberareloo.png",
      "skin/text.png",
      "skin/tick.png",
      "skin/video.png",
      "skin/webhook.png",
      "skin/win/close.png"
   ],
   "icons": {
     "128": "skin/fork128.png",
     "64": "skin/fork64.png",
     "32": "skin/fork32.png",
     "16": "skin/fork16.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "23.0",
   "default_locale": "en_US",
   "description": "Yet Another Tombloo on Chromium",
   "name": "Taberareloo",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "bookmarks", "contextMenus", "cookies", "unlimited_storage", "notifications", "webRequest" ],
   "version": "2.0.73",
   "incognito": "split"
}
